<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>homepage.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>save</servlet-name>
        <servlet-class>com.servlets.save</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>client</servlet-name>
        <servlet-class>com.servlets.client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lawyer</servlet-name>
        <servlet-class>com.servlets.lawyer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Savee</servlet-name>
        <servlet-class>com.servlets.Savee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sav</servlet-name>
        <servlet-class>com.servlets.Sav</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addcase</servlet-name>
        <servlet-class>com.servlets.addcase</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save</servlet-name>
        <url-pattern>/save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>client</servlet-name>
        <url-pattern>/client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lawyer</servlet-name>
        <url-pattern>/lawyer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Savee</servlet-name>
        <url-pattern>/Savee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sav</servlet-name>
        <url-pattern>/Sav</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addcase</servlet-name>
        <url-pattern>/addcase</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
